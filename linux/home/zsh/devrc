# vim:ft=zsh:

if [ -d ${HOME}/perl5 ]
then
    source ${HOME}/perl5/perlbrew/etc/bashrc
fi

if [ -d ${HOME}/.pythonbrew ]
then
    source ${HOME}/.pythonbrew/etc/bashrc
fi

if [ -d ${HOME}/.rvm ]
then
    source "$HOME/.rvm/scripts/rvm" # Load RVM function
fi

if [ -d $HOME/.nvm ]
then
    source ~/.nvm/nvm.sh
fi

