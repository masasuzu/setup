path=(
    {/usr/{/local,},}/{,s}bin
)

# fix $HOST
case "${HOST/.*/}" in
philippe)
    DOMAIN="[ LOCAL ]"
    ;;
leon)
    DOMAIN="[ INTRA ]"
    ;;
lenny*|squeeze*)
    DOMAIN="[ LAB ]"
    ;;
lab)
    DOMAIN="[ VPS-LAB ]"
    ;;
*)
    print ".zsh/devprofile: $HOST: unkown hostname"
    ;;
esac
export DOMAIN
