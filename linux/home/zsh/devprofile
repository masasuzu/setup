# vim:ft=zsh:

# fix $HOST
case "${HOST/.*/}" in
philippe|henry)
    DOMAIN="[ LOCAL ]"
    ;;
leon)
    DOMAIN="[ INTRA ]"
    ;;
eve|marie)
    DOMAIN="[ VM-INTRA ]"
    ;;
lenny*|squeeze*)
    DOMAIN="[ LAB ]"
    ;;
lab)
    DOMAIN="[ VPS-LAB ]"
    ;;
*)
    print ".zsh/devprofile: $HOST: unkown hostname"
    DOMAIN="[ UNKNOWN ]"
    ;;
esac
export DOMAIN
